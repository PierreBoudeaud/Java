import java.util.ArrayList;

public class Classe {
	private int effectifMax;
	//Création de la liste
	private ArrayList<Eleve> classe;
	
	//Constructeur de l'objet Classe
	public Classe(){
		this.effectifMax = 4;
		//On crée l'espace mémoire pour la liste classe
		this.classe = new ArrayList<Eleve>();
	}
	
	//Ajoute un élève dans classe
	public void ajoutEleve(Eleve el){
		this.classe.add(el);
	}
	
	
}
